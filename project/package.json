{
  "name": "ktlbd-website-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier -w .",
    "clean": "rimraf .next dist build coverage",
    "preview": "vite preview",
    "start": "vite preview",
    "build:wsl": "node node_modules/vite/bin/vite.js build --outDir $HOME/WEB20_project_dist",
    "preview:wsl": "node node_modules/vite/bin/vite.js preview --port 4173 --strictPort --outDir $HOME/WEB20_project_dist"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.6"
  }
}
